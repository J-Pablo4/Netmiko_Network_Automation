from netmiko import ConnectHandler

# Create connection object
device = {
    'device_type': 'cisco_ios_telnet',
    'ip': '192.168.232.159',
    'username': 'cisco',
    'password': 'cisco',
    'secret': 'cisco',
    'port': 23, # Telnet port
}

net_connect = ConnectHandler(**device)

net_connect.enable()

output = net_connect.send_command('show running-config')

print(output)

net_connect.disconnect()
